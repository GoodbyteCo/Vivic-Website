---
import MainCentered from '../layouts/MainCentered.astro'
import Work from '../components/Work.vue'
const data = Astro.fetchContent('../work/*.md')
const work = data.map((post) => { return { title: post.title, pdf: post.pdf, work: post.work, topic: post.topic, source: post.astro.source, url: post.url }})
--- 


<MainCentered>
    <Work works={work} client:load />
</MainCentered>

